import base64

# Ruta local de la imagen (ajustala si es necesario)
with open("voltaris-logo-dragon.png", "rb") as image_file:
    encoded = base64.b64encode(image_file.read()).decode()

# Generar c√≥digo HTML para insertar la imagen directamente
html_code = f'<img src="data:image/png;base64,{encoded}" alt="Voltaris Logo" style="width:200px;">'

# Guardar en archivo o imprimir
print(html_code)
